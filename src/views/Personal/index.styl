.personal_content_wrapper {
  width: 100%;
  position: relative;
  ms('min-height', 63vh 60vh 64vh 68vh 62vh 62vh);

  .personal_warp {
    width: 100%;
    position: relative;
    max-width: 1250px;
    padding: 0 10px;
    margin: 0 auto;

    .personal_content {
      width: 100%;
      position: relative;
      ms('padding-top', 28px 58px 78px 88px 98px 104px);
      ms('padding-bottom', 44px 64px 84px 104px 124px 144px);

      .person_name_box {
        border-radius: 12px;
        border: 1px solid #000000;
        width: 100%;
        ms('padding-top', 12px 22px 32px 42px 42px 42px);
        ms('padding-bottom', 12px 22px 32px 42px 42px 42px);
        ms('padding-left', 12px 22px 32px 42px 42px 42px);
        ms('padding-right', 12px 22px 32px 42px 42px 42px);
        display: flex;
        align-items: center;
        justify-content: space-between;
        ms('margin-bottom', 23px 28px 33px 38px 43px 43px);

        .name_box {
          display: flex;
          align-items: center;
          ms('width', 55% 45% 45% 45% 45% 45%);

          .name_img {
            ms('width', 58px 78px 98px 118px 138px 138px);

            img {
              width: 100%;
              display: block;
            }
          }

          .name_text {
            ms('margin-left', 5px 6px 7px 8px 9px 10px);

            h2 {
              display: flex;
              align-items: center;

              em {
                ms('font-size', 16px 18px 20px 22px 24px 26px);
                font-family: Source Han Sans CN-Bold, Source Han Sans CN;
                font-weight: bold;
                color: #1A1210;
                position: relative;

                &:before {
                  position: absolute;
                  left: 0;
                  bottom: -2px;
                  content: '';
                  width: 100%;
                  height: 1px;
                  background: #1A1210;
                }
              }

              a {
                margin-left: 10px;
                font-family: Source Han Sans CN-Bold, Source Han Sans CN;
                font-weight: bold;
                color: #1A1210;
                ms('font-size', 12px 13px 14px 15px 16px 16px);
                transition();

                &:hover {
                  opacity: 0.8;
                }
              }
            }

            span {
              display: block;
              ms('margin-top', 10px 11px 12px 13px 14px 14px);
              ms('margin-bottom', 4px 5px 6px 7px 8px 8px);
              font-family: Arial-Bold, Arial;
              font-weight: bold;
              color: #1A1210;
              ms('font-size', 13px 13px 14px 15px 16px 16px);
            }

            p {
              ms('font-size', 12px 12px 12px 13px 13px 13px);
            }
          }
        }

        .address_box {
          ms('width', 40% 50% 50% 50% 50% 50%);
          display: flex;
          justify-content: right;
          flex-wrap: wrap;

          .invite_button {
            display: flex;
            align-items: center;
            width: 100%;
            justify-content: right;
            margin-bottom: 4px;

            .base_components_button_layout {
              // ms('min-height', 33px 42px 44px 46px 48px 48px);
              min-height: 33px !important;
              font-size: 14px !important;
              padding-left: 10px;
              padding-right: 10px;
              width: auto;
            }

            span {
              ms('font-size', 12px 12px 13px 13px 14px 14px);
              // ms('padding-right', 8px 9px 10px 11px 12px 13px);
              text-align: right;
            }
          }

          .invite_text {
            width: 100%;
            display: flex;
            align-items: center;
            justify-content: right;
            font-family: Arial-Regular, Arial;
            font-weight: 400;
            color: #1A1210;
            ms('font-size', 12px 12px 13px 13px 14px 14px);
            padding: 5px 0;
            cursor: pointer;

            img {
              ms('width', 12px 12px 12px 12px 12px 12px);
              display: block;
              ms('margin-left', 5px 6px 7px 8px 9px 9px);
            }
          }
        }
      }
    }
  }

  .center_record_wrap {
    width: 100%;
    position: relative;

    .record_nav_list {
      width: 100%;
      position: relative;
      display: flex;
      border-left: 1px solid #000000;
      ms('margin-bottom', 10px 14px 18px 22px 26px 30px);

      .record_nav {
        display: flex;
        flex: 1;
        align-items: center;
        justify-content: center;
        border: 1px solid #000000;
        border-left: none;
        ms('padding-top', 8px 9px 10px 11px 12px 15px);
        ms('padding-bottom', 8px 9px 10px 11px 12px 15px);
        ms('font-size', 12px 13px 14px 15px 16px 16px);
        font-family: 'Source Han Sans CN-Regular';
        font-weight: 400;
        color: #000000;
        transition();

        span {
          ms('transform', scale(0.875) none none none none none);
        }

        &:hover, &.on {
          background: #000;
          color: #fff;
        }
      }
    }

    .record_content_list {
      width: 100%;
      position: relative;

      .record_content {
        width: 100%;
        position: relative;

        .assets_record_wrap {
          width: 100%;
          position: relative;

          .second_nav_wrap {
            width: 100%;
            position: relative;
            overflow: hidden;
            ms('margin-bottom', 10px 14px 18px 22px 26px 30px);

            a {
              float: left;
              width: 50%;
              ms('max-width', none 138px 138px 138px 138px 138px);
              text-align: center;
              border: 1px solid #000000;
              ms('padding-top', 8px 9px 10px 11px 12px 15px);
              ms('padding-bottom', 8px 9px 10px 11px 12px 15px);
              ms('font-size', 12px 13px 14px 15px 16px 16px);
              font-family: 'Source Han Sans CN-Regular';
              font-weight: 400;
              color: #000000;
              transition();
              box-sizing: border-box;

              &:last-child {
                border-left: none;
              }

              &:hover, &.on {
                background: #000;
                color: #fff;
              }

              span {
                ms('transform', scale(0.875) none none none none none);
              }
            }
          }
        }
      }
    }

    .record_detail_wrap {
      width: 100%;
      position: relative;
      ms('min-height', 229px 280px 280px 317px 350px 350px);

      .item_list {
        width: 100%;
        position: relative;
      }

      &.recode_inner {
        border-radius: 12px;
        border: 1px solid #000000;
        ms('padding-top', 10px 11px 12px 13px 14px 15px);
        ms('padding-bottom', 19px 21px 23px 25px 27px 29px);
        ms('padding-left', 10px 20px 30px 40px 50px 50px);
        ms('padding-right', 10px 20px 30px 40px 50px 50px);

        .record_detail {
          padding: 0 !important;
          border: none;
        }
      }

      &.wait_record {
        display: flex;
        justify-content: center;
        align-items: center;
        ms('font-size', 14px 16px 18px 22px 22px 23px);
      }

      .record_detail {
        width: 100%;
        position: relative;
        border-radius: 12px;
        border: 1px solid #000000;
        ms('padding-top', 10px 11px 12px 13px 14px 15px);
        ms('padding-bottom', 19px 21px 23px 25px 27px 29px);
        ms('padding-left', 10px 20px 30px 40px 50px 50px);
        ms('padding-right', 10px 20px 30px 40px 50px 50px);

        // ms('min-height')
        .base_components_list_layout {
          ms('min-height', 229px 280px 280px 317px 350px 350px);
        }

        .operate {
          ms('display', block block flex flex flex flex);

          .base_components_button_layout {
            width: auto;
            margin: 2px;
            ms('font-size', 12px 13px 16px 17px 18px 19px);
            ms('min-height', 24px 32px 44px 46px 48px 48px);
          }
        }
      }

      .text_title {
        width: 100%;
        position: relative;

        h2 {
          ms('font-size', 14px 16px 18px 22px 22px 23px);
          font-family: Poppins-Bold, Poppins;
          font-weight: bold;
          color: #1A1210;
          line-height: 1.5;
          ms('margin-top', 6px 14px 16px 18px 20px 22px);
        }

        h4 {
          ms('font-size', 14px 16px 18px 22px 22px 23px);
          font-family: Poppins-Bold, Poppins;
          font-weight: bold;
          color: #1A1210;
          line-height: 1.5;
        }

        h3 {
          ms('font-size', 13px 14px 15px 16px 16px 16px);
          font-family: 'Source Han Sans CN-Regular';
          font-weight: 400;
          color: #000000;
          line-height: 1.5;
          ms('margin-top', 6px 12px 14px 16px 18px 18px);
          ms('margin-bottom', 2px 3px 4px 5px 6px 6px);
          display: block;
        }

        h5 {
          ms('font-size', 13px 14px 15px 16px 16px 16px);
          font-family: 'Source Han Sans CN-Regular';
          font-weight: 400;
          color: #000000;
          line-height: 1.5;
          ms('margin-bottom', 6px 12px 14px 16px 18px 18px);
          ms('margin-top', 2px 3px 4px 5px 6px 6px);
          display: block;

          span {
            color: #32A6FF;
            font-weight: bold;
          }
        }

        .text_nav_wrap {
          width: 100%;
          position: relative;

          .text_nav {
            display: inline-block;
            ms('padding-top', 8px 8px 10px 10px 12px 12px);
            ms('padding-bottom', 8px 8px 10px 10px 12px 12px);
            ms('padding-left', 10px 12px 14px 16px 18px 18px);
            ms('padding-right', 10px 12px 14px 16px 18px 18px);
            ms('margin-top', 10px 12px 14px 16px 18px 18px);
            ms('margin-bottom', 10px 12px 14px 16px 18px 18px);
            background: #fff;
            color: #000;
            ms('font-size', 12px 12px 13px 13px 13px 13px);
            font-family: 'Source Han Sans CN-Regular';
            font-weight: 400;
            border: 1px solid #000000;
            position: relative;
            margin-left: -1px;

            &:hover, &.on {
              background: #000;
              color: #fff;
              z-index: 1;
            }

            &:first-child {
              margin-left: 0;
            }
          }
        }

        .text_secode_nav {
          width: 100%;
          position: relative;
          overflow: hidden;
          ms('margin-bottom', 10px 14px 18px 22px 26px 30px);

          a {
            float: left;
            width: 33.33%;
            ms('max-width', none 138px 138px 138px 138px 138px);
            text-align: center;
            border: 1px solid #ACACAC;
            ms('padding-top', 8px 9px 10px 11px 12px 15px);
            ms('padding-bottom', 8px 9px 10px 11px 12px 15px);
            ms('font-size', 12px 13px 14px 15px 16px 16px);
            font-family: 'Source Han Sans CN-Regular';
            font-weight: 400;
            color: #ACACAC;
            transition();
            box-sizing: border-box;
            margin-left: -1px;
            position: relative;

            &:hover, &.on {
              border-color: #000;
              color: #000000;
              z-index: 1;
            }

            &:first-child {
              margin-left: 0;
            }

            span {
              ms('transform', scale(0.875) none none none none none);
            }
          }
        }
      }
    }
  }
}

.nick_box_wrap {
  .base_components_input_layout {
    ms('margin-bottom', 20px 30px 25px 25px 30px 30px);
  }
}